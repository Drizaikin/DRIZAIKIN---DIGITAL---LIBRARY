{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/ingest/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/ingest/trigger.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/admin/health/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/admin/health",
      "dest": "/api/admin/health/index.js"
    },
    {
      "src": "/api/ingest/trigger",
      "dest": "/api/ingest/trigger.js"
    },
    {
      "src": "/api/ingest",
      "dest": "/api/ingest/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "crons": [
    {
      "path": "/api/ingest",
      "schedule": "0 3 * * *"
    }
  ]
}
